          yyline++;
          yycolumn = 0;
          zzR = false;
          break;
        case '\r':
          yyline++;
          yycolumn = 0;
          zzR = true;
          break;
        case '\n':
          if (zzR)
            zzR = false;
          else {
            yyline++;
            yycolumn = 0;
          }
          break;
        default:
          zzR = false;
          yycolumn++;
        }
      }

      if (zzR) {
        // peek one character ahead if it is \n (if we have counted one line too much)
        boolean zzPeek;
        if (zzMarkedPosL < zzEndReadL)
          zzPeek = zzBufferL[zzMarkedPosL] == '\n';
        else if (zzAtEOF)
          zzPeek = false;
        else {
          boolean eof = zzRefill();
          zzEndReadL = zzEndRead;
          zzMarkedPosL = zzMarkedPos;
          zzBufferL = zzBuffer;
          if (eof) 
            zzPeek = false;
          else 
            zzPeek = zzBufferL[zzMarkedPosL] == '\n';
        }
        if (zzPeek) yyline--;
      }
      zzAction = -1;

      zzCurrentPosL = zzCurrentPos = zzStartRead = zzMarkedPosL;
  
      zzState = ZZ_LEXSTATE[zzLexicalState];


      zzForAction: {
        while (true) {
    
          if (zzCurrentPosL < zzEndReadL)
            zzInput = zzBufferL[zzCurrentPosL++];
          else if (zzAtEOF) {
            zzInput = YYEOF;
            break zzForAction;
          }
          else {
            // store back cached positions
            zzCurrentPos  = zzCurrentPosL;
            zzMarkedPos   = zzMarkedPosL;
            boolean eof = zzRefill();
            // get translated positions and possibly new buffer
            zzCurrentPosL  = zzCurrentPos;
            zzMarkedPosL   = zzMarkedPos;
            zzBufferL      = zzBuffer;
            zzEndReadL     = zzEndRead;
            if (eof) {
              zzInput = YYEOF;
              break zzForAction;
            }
            else {
              zzInput = zzBufferL[zzCurrentPosL++];
            }
          }
          int zzNext = zzTransL[ zzRowMapL[zzState] + zzCMapL[zzInput] ];
          if (zzNext == -1) break zzForAction;
          zzState = zzNext;

          int zzAttributes = zzAttrL[zzState];
          if ( (zzAttributes & 1) == 1 ) {
            zzAction = zzState;
            zzMarkedPosL = zzCurrentPosL;
            if ( (zzAttributes & 8) == 8 ) break zzForAction;
          }

        }
      }

      // store back cached position
      zzMarkedPos = zzMarkedPosL;

      switch (zzAction < 0 ? zzAction : ZZ_ACTION[zzAction]) {
        case 53: 
          { return newToken(Terminals.RETURN, yytext());
          }
        case 55: break;
        case 1: 
          { throw new Scanner.Exception("carácter inesperado na entrada: '" + yytext() + "'");
          }
        case 56: break;
        case 10: 
          { return newToken(Terminals.COMMA, yytext());
          }
        case 57: break;
        case 30: 
          { return newToken(Terminals.TYBIND, yytext());
          }
        case 58: break;
        case 48: 
          { return newToken(Terminals.TYBOOL, yytext());
          }
        case 59: break;
        case 33: 
          { return newToken(Terminals.NEQ, yytext());
          }
        case 60: break;
        case 16: 
          { return newToken(Terminals.RPAREN, yytext());
          }
        case 61: break;
        case 17: 
          { return newToken(Terminals.PLUS, yytext());
          }
        case 62: break;
        case 2: 
          { /* Ignorar  */
          }
        case 63: break;
        case 31: 
          { return newToken(Terminals.RTR, yytext());
          }
        case 64: break;
        case 23: 
          { return newToken(Terminals.ATTR, yytext());
          }
        case 65: break;
        case 28: 
          { yybegin(YYINITIAL); return newToken(Terminals.CHAR, c);
          }
        case 66: break;
        case 50: 
          { return newToken(Terminals.NULL, yytext());
          }
        case 67: break;
        case 12: 
          { return newToken(Terminals.RBRACE, yytext());
          }
        case 68: break;
        case 13: 
          { return newToken(Terminals.LBRACKET, yytext());
          }
        case 69: break;
        case 44: 
          { return newToken(Terminals.ELSE, yytext());
          }
        case 70: break;
        case 3: 
          { return newToken(Terminals.SUB, yytext());
          }
        case 71: break;
        case 54: 
          { return newToken(Terminals.ITERATE, yytext());
          }
        case 72: break;
        case 49: 
          { return newToken(Terminals.TYVOID, yytext());
          }
        case 73: break;
        case 26: 
          { c = '\0'; yybegin(CHAR);
          }
        case 74: break;
        case 38: 
          { c = '\n';
          }
        case 75: break;
        case 7: 
          { return newToken(Terminals.DOT, yytext());
          }
        case 76: break;
        case 42: 
          { return newToken(Terminals.NEW, yytext() );
          }
        case 77: break;
        case 19: 
          { return newToken(Terminals.DIV, yytext());
          }
        case 78: break;
        case 29: 
          { c = '\\';
          }
        case 79: break;
        case 21: 
          { return newToken(Terminals.NOT, yytext());
          }
        case 80: break;
        case 15: 
          { return newToken(Terminals.LPAREN, yytext());
          }
        case 81: break;
        case 25: 
          { return newToken(Terminals.FALSE, yytext());
          }
        case 82: break;
        case 4: 
          { return newToken(Terminals.ID, yytext());
          }
        case 83: break;
        case 32: 
          { return newToken(Terminals.AND, yytext());
          }
        case 84: break;
        case 11: 
          { return newToken(Terminals.LBRACE, yytext());
          }
        case 85: break;
        case 43: 
          { return newToken(Terminals.TYINT, yytext());
          }
        case 86: break;
        case 14: 
          { return newToken(Terminals.RBRACKET, yytext());
          }
        case 87: break;
        case 39: 
          { c = '\t';
          }
        case 88: break;
        case 20: 
          { return newToken(Terminals.MOD, yytext());
          }
        case 89: break;
        case 45: 
          { return newToken(Terminals.READ, yytext());
          }
        case 90: break;
        case 5: 
          { return newToken(Terminals.TYID, yytext());
          }
        case 91: break;
        case 51: 
          { return newToken(Terminals.WHILE, yytext());
          }
        case 92: break;
        case 46: 
          { return newToken(Terminals.DATA,yytext());
          }
        case 93: break;
        case 40: 
          { c = '\b';
          }
        case 94: break;
        case 47: 
          { return newToken(Terminals.TYCHAR, yytext());
          }
        case 95: break;
        case 35: 
          { return newToken(Terminals.EQ, yytext());
          }
        case 96: break;
        case 52: 
          { return newToken(Terminals.PRINT, yytext());
          }
        case 97: break;
        case 27: 
          { c = yytext().charAt(0);
          }
        case 98: break;
        case 24: 
          { return newToken(Terminals.TRUE, yytext());
          }
        case 99: break;
        case 41: 
          { return newToken(Terminals.FLOAT, new Float(yytext()) );
          }
        case 100: break;
        case 37: 
          { c = '\r';
          }
        case 101: break;
        case 18: 
          { return newToken(Terminals.MULT, yytext());
          }
        case 102: break;
        case 6: 
          { return newToken(Terminals.INT, new Integer(yytext()));
          }
        case 103: break;
        case 8: 
          { return newToken(Terminals.SEMI, yytext());
          }
        case 104: break;
        case 22: 
          { return newToken(Terminals.LT, yytext());
          }
        case 105: break;
        case 36: 
          { return newToken(Terminals.IF, yytext());
          }
        case 106: break;
        case 9: 
          { return newToken(Terminals.COLON, yytext());
          }
        case 107: break;
        case 34: 
          { return newToken(Terminals.RTL, yytext());
          }
        case 108: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
              {          return newToken(Terminals.EOF, "end-of-file");
 }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
